%h4 Create an account
= form_for(resource, :as => resource_name, :url => registration_path(resource_name), :html => {:class => "span8 well"}) do |f| 
  = devise_error_messages! 

  = f.label :name 
  = f.text_field :name

  = f.label :email
  = f.email_field :email

  = f.label :password
  = f.password_field :password

  = f.label :password_confirmation
  = f.password_field :password_confirmation

  = button_tag(type: 'submit', class: "btn btn-primary", id: "project-submit") do
    Test

/ = render "devise/shared/links"
