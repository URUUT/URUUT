    %meta{:name => "viewport", :content => "width=device-width, initial-scale=1, maximum-scale=1"}
    %meta{name: "google-site-verification", content: "-tT5oeE5O8Z7igfTKvt90NLf8iY5wKzbZmKh7RReVpA"}
    %title= content_for?(:title) ? yield(:title) : "URUUT"
    %meta{:content => "", :name => "description"}
    %meta{:content => "", :name => "author"}
    = favicon_link_tag 'favicon.ico'
    = stylesheet_link_tag "login/application-login", "http://fonts.googleapis.com/css?family=Lato:300,400,700,400italic", :media => "all"
    /[if IE]
      = stylesheet_link_tag "ie-only.css.scss"
    = javascript_include_tag "application"
    = yield(:head)

    %body
      .login-logo
        = image_tag 'logo-name.png'
      = yield

